{
  "apps": [
    {
      "name": "epaper-work-api",
      "script": "app.js",
      "node_args": "-harmony",
      "watch": true,
      "exec_mode": "cluster",
      "instances": 2,
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "env": {
        "NODE_ENV": "production"
      },
      "env_dev": {
        "NODE_ENV": "development",
        "PORT": "8001"
      },
      "env_test": {
        "NODE_ENV": "test",
        "PORT": "8002"
      }
    }
  ]
}